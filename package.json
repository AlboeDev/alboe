{
  "name": "alboe",
  "packageManager": "yarn@4.0.0-rc.33",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0",
    "yarn": ">=3.0.0"
  },
  "workspaces": [
    "./packages/**"
  ],
  "scripts": {
    "@workspaces": "yarn workspaces foreach --topological-dev --parallel",
    "@all": "yarn @workspaces --from \"@alboe/**\" run",
    "@config": "yarn @workspaces --from \"@alboe/config-*\" run",
    "@config:eslint": "yarn workspace @alboe/config-eslint",
    "@config:typescript": "yarn workspace @alboe/config-typescript",
    "build": "yarn @all build",
    "clean": "yarn @all clean"
  },
  "devDependencies": {
    "eslint": "^8.29.0",
    "typescript": "^4.9.4"
  }
}
