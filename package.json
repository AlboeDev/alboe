{
  "name": "alboe",
  "packageManager": "yarn@3.4.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/alboe-development/alboe.git"
  },
  "license": "MIT",
  "engines": {
    "node": "18",
    "npm": "8",
    "yarn": "3"
  },
  "workspaces": [
    "packages/**/*"
  ],
  "scripts": {
    "@workspaces": "yarn workspaces foreach --parallel --topological-dev --verbose",
    "@all": "yarn @workspaces run",
    "@config": "yarn @workspaces --from \"@alboe/*-config\" run"
  },
  "devDependencies": {
    "eslint": "8.43.0",
    "typescript": "5.1.3"
  }
}
