{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.rulers": [
    80,
    100,
    120
  ],
  "editor.tabSize": 2,
  "eslint.nodePath": ".yarn/sdks",
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "files.associations": {
    "*.json": "jsonc"
  },
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.quoteStyle": "single"
}
