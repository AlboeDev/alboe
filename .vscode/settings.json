{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.rulers": [
    80,
    100,
    120
  ],
  "editor.tabSize": 2,
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "files.associations": {
    "*.json": "jsonc"
  },
  "search.exclude": {
    "**/.yarn": true,
  },
  "typescript.preferences.quoteStyle": "single"
}
