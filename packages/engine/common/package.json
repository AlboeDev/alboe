{
  "name": "@alboe/engine-common",
  "packageManager": "yarn@3.4.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/alboe-development/alboe.git"
  },
  "license": "MIT",
  "engines": {
    "node": "18",
    "npm": "8",
    "yarn": "3"
  },
  "files": [
    "./dist/*"
  ],
  "main": "./dist/module/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "yarn build:module && yarn build:types",
    "build:module": "tsc --declaration false --outDir ./dist/module",
    "build:types": "tsc --declaration --declarationMap --declarationDir ./dist/types --emitDeclarationOnly",
    "clean": "yarn clean:module && yarn clean:types",
    "clean:module": "rimraf ./dist/module",
    "clean:types": "rimraf ./dist/types",
    "test": "yarn test:style && yarn test:syntax",
    "test:style": "eslint \"./{src,static,test}/**/*.{js,ts}\"",
    "test:style:fix": "yarn test:style --fix",
    "test:syntax": "tsc --noEmit"
  },
  "devDependencies": {
    "@alboe/eslint-config": "workspace:^",
    "@alboe/typescript-config": "workspace:^",
    "eslint": "8.43.0",
    "rimraf": "^5.0.1",
    "typescript": "4.5.2"
  }
}
